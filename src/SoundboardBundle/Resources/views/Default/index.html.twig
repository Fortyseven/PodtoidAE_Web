    <div id="CarouselContainer">
        {% for category in data.category %}
            <div class="page">
                <div id="CategoryBanner">
                    <img src={{ asset('assets/images/header_sterling.png') }}/>
                </div>
                <div id="ButtonContainer">
                    <ul>
                        {% for clip in category.clips %}
                            <li class="sound_button" onclick="soundboard.PlaySound({{ clip.path }})">{{ clip.title }}</li>
                        {% endfor %}
                    </ul>
                </div>
            </div>

